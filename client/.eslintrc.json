{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      1, { "extensions": [".js", ".jsx"] }
    ],
    "react/prop-types": [
      0, { }
    ],
    "react/no-array-index-key": [
      0, { }
    ],
    "padded-blocks": [
      "error",
      { "blocks": "never" }
    ]
  }
}
